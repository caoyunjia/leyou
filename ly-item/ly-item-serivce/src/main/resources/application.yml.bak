server:
  port: 8001
spring:
  datasource:
    url: jdbc:mysql://192.168.1.8/leyou
    username: root
    password: root
    driver-class-name: com.mysql.jdbc.Driver
  rabbitmq:
    host: 192.168.1.8
    username: leyou
    password: leyou
    virtual-host: /leyou
    template:
      retry:
        enabled: true
        initial-interval: 10000ms
        max-interval: 30000m
        multiplier: 2
      exchange: ly.item.exchange
    publisher-confirms: true
info:
  app.name: ly-item
  company.name: www.leyou.com
  build.artifactId: $project.artifactId$
  build.version: $project.version$
mybatis:
  configuration:
    log-impl: org.apache.ibatis.logging.stdout.StdOutImpl